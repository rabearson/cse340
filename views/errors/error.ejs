<section class="error-section">
    <!-- <h1><%= title %></h1> -->
     <% if (title == 404 || title === '404') { %>
    <img src="/images/404.svg" alt="<%- message %>" class="error-image">
    <% } else {%>
     <img src="/images/404-1.svg" alt="<%- message %>" class="error-image">
     <% } %>
    <p class="error-message"><%- message %></p>
</section>